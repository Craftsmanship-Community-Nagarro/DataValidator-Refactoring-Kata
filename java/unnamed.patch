@@ -0,0 +1,10 @@
+package com.craftmanship;
+
+import java.util.function.Predicate;
+
+public class DataPredicates {
+
+    public static Predicate<Data> isNameValid() {
+        return p -> !p.getFirstName().isBlank()  &&  p.getFirstName().chars().allMatch(Character::isAlphabetic);
+    }
+}
